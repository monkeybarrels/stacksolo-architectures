      {
        "name": "main",
        "functions": [
          {
            "name": "api",
            "runtime": "nodejs20",
            "entryPoint": "handler",
            "memory": "256Mi",
            "allowUnauthenticated": true
          }
        ]
      }
    ]
  }
}